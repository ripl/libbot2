for d in . .. ../.. ../../.. ../../../.. "@CMAKE_INSTALL_PREFIX@"; do
  if [ -d $d/share/java ]; then
    jd=$d/share/java
    echo Checking $jd
    for f in $jd/lcmtypes_*.jar $jd/lcmspy_plugins_*.jar; do
      if [ -e $f ]; then
        echo "   Found $f"
        CLASSPATH="${CLASSPATH:+$CLASSPATH:}$f"
      fi
    done
  fi
done
export CLASSPATH
exec "@LCM_SPY_EXECUTABLE@"
